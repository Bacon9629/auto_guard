<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!-- 顏色 -->
    <solid android:color="#0018506C" />

    <!-- 設置圓角 -->
    <corners android:radius="25dp" />
